.header_outher
  z-index 10
  position absolute
  right 0
  left 0
  height 50px
  line-height 50px
  padding 0 12px
  text-align center
  -webkit-backface-visibility hidden
  backface-visibility hidden
  display flex
  section.home_left
    width 25%
    flex 0 0 25%
    .home_back_icon
      width 50px
      height 50px
      position relative
      .icon_Back
        width 20px
        height 28px
        display block
        position absolute
        top 10px
        svg
          width 100%
          height 100%
          fill #fff
  section.home_center
    width 50%
    flex 0 0 50%
  section.home_right
    width 25%
    flex 0 0 25%
    p
      color #ffffff
      font-size 14px
