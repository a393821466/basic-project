html, body, #app
  height 100%
  width 100%
  border 0
  font-size 14px
  overflow-x hidden
  margin 0
  padding 0
body
  font-family 'Microsoft YaHei'
  font-family Arial, Helvetica, sans-serif
  box-sizing border-box
div,p,ul,li,ol,dl,dt,dd,span,img,input,table,h1,h2,h3,h4,h5,h6
  margin:0
  padding:0
  border:0
  list-style:none
  list-style-type:none
i,em
  font-style:normal
a
  text-decoration:none
a:visited
  text-decoration:none
